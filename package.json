{
  "name": "java-compiler",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=development vue-cli-service serve",
    "build": "cross-env NODE_ENV=production vue-cli-service build --modern",
    "deploy": "cross-env NODE_DEBUG=gh-pages gh-pages -d dist -b gh-pages -t",
    "publish": "npm run build && npm run deploy",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "win32": "electron-installer-windows --src dist_electron/win-unpacked/ --dest dist_electron/installers/ --arch amd64 --config src_electron/config.json --asar",
    "deb64": "electron-installer-debian --src ./dist_electron/linux-unpacked/ --dest dist_electron/installers/ --arch amd64 --config src_electron/config.json",
    "pack:debian": "npm run electron:build && cp -a ./src_electron/resources/. ./dist_electron/linux-unpacked/ && npm run deb64",
    "pack:win": "npm run electron:build && npm run win32"
  },
  "main": "background.js",
  "dependencies": {
    "buefy": "^0.8.0",
    "core-js": "^3.6.5",
    "electron-installer-debian": "^3.1.0",
    "electron-installer-windows": "^3.0.0",
    "file-saver": "^2.0.2",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "xlsx": "^0.16.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "axios": "^0.18.0",
    "cross-env": "^7.0.2",
    "electron": "^11.5.0",
    "electron-devtools-installer": "^3.1.0",
    "gh-pages": "^3.1.0",
    "node-sass": "^4.12.0",
    "raw-loader": "^4.0.2",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-buefy": "~0.3.7",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.4",
    "vue-template-compiler": "^2.6.11"
  }
}
